<?xml version="1.0" encoding="utf-8"?>
<resources>
    <data-extraction-rules>
        <cloud-backup disableIfNoEncryptionCapabilities="true">
            <include domain="file" path="."/>
            <exclude domain="sharedpref" path="."/>
        </cloud-backup>
        <device-transfer>
            <include domain="file" path="."/>
            <exclude domain="sharedpref" path="."/>
        </device-transfer>
    </data-extraction-rules>
</resources>
