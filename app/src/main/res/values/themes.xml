<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.SafeTrace" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/safe_primary</item>
        <item name="colorPrimaryVariant">@color/safe_primary_variant</item>
        <item name="colorOnPrimary">@color/safe_on_primary</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/safe_secondary</item>
        <item name="colorSecondaryVariant">@color/safe_secondary_variant</item>
        <item name="colorOnSecondary">@color/safe_on_secondary</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="android:windowBackground">@color/safe_background</item>
    </style>
</resources>
